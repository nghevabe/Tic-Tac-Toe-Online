<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.projecttictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".MyService"
            android:enabled="true" />

        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GamePlay" />
        <activity android:name=".Register" />
        <activity android:name=".SelectMode" />
        <activity android:name=".OnlineServer" />
        <activity android:name=".WaitingRoom" />
        <activity android:name=".OnlineGamePlay" />
        <activity android:name=".ResultRoom" />
        <activity android:name=".WaitingResponse" />
        <activity android:name=".MatchMaking"></activity>
    </application>

</manifest>